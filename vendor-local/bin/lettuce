#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'lettuce==0.1.34','console_scripts','lettuce'
__requires__ = 'lettuce==0.1.34'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('lettuce==0.1.34', 'console_scripts', 'lettuce')()
    )
